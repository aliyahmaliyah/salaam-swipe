<div class="newsSnippet">
    <div class="container">
        <div class="row">
            <div class='col-lg-12 col-md-12'>
                <h1>News</h1>
            </div>
            <div class='col-lg-8 col-md-8'>
                <ul>
                <% site.posts.forEach(function(post) { %>
                    <li>
                        <h2>
                            <a href="<%= post.permalink %>">
                                <%= post.title %>
                            </a>
                        </h2>
                    </li>
                    <p><%-post.excerpt%></p>
            </div>
            <div class='col-lg-4 col-md-4 photo'>
                <p><%-page.photos%></p>
            </div>
                <% }) %>
                </ul>
        </div>
    </div>
</div>